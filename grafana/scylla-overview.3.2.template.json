                            {
                                "class": "hidden_column",
                                "pattern": "cluster"
                            },
                            {
                                "class": "hidden_column",
                                "pattern": "monitor"
                            },
                            {
                                "class": "hidden_column",
                                "pattern": "summary"
                            },
                            {
                                "alias": "Instance",
                                "colorMode": null,
                                "colors": [
                                    "rgba(245, 54, 54, 0.9)",
                                    "rgba(237, 129, 40, 0.89)",
                                    "rgba(50, 172, 45, 0.97)"
