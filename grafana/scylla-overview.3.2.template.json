                                "link": true,
                                "linkTooltip": "Jump to the see the node",
                                "linkUrl": "/d/detailed-[[dash_version]]/detailed?refresh=30s&orgId=1&var-by=instance&var-node=${__cell}",
                                "mappingType": 1,
